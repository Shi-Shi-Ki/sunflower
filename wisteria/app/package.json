{
  "name": "memos",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "storybook": "start-storybook -p 8081 -c .storybook",
    "buid-storybook": "build-storybook -s public",
    "lint:js": "eslint --fix --ext .vue --ignore-path .gitignore .",
    "test": "jest",
    "chromatic": "npx chromatic --project-token ef8830e837a8"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "js"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@vue/cli": "^4.5.9",
    "nuxt": "^2.14.7",
    "vuex-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.7",
    "@nuxt/types": "^2.14.7",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-controls": "^6.0.28",
    "@storybook/addon-docs": "^6.0.28",
    "@storybook/addon-knobs": "^6.1.2",
    "@storybook/addon-storysource": "^6.0.28",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/source-loader": "^6.0.28",
    "@storybook/vue": "^6.0.28",
    "@typescript-eslint/parser": "^4.8.1",
    "@vue/composition-api": "^1.0.0-beta.19",
    "@vue/test-utils": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-require-context-hook": "^1.0.0",
    "chromatic": "^5.6.3",
    "core-js": "^3.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-webpack-plugin": "^6.0.2",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "sass-loader": "^8.0.2",
    "vue-jest": "^3.0.7",
    "vue-svg-loader": "^0.16.0"
  }
}
