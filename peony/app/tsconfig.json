{
    "compilerOptions": {
        "outDir": "/dist",
        "sourceMap": true,
        "module": "commonjs",
        "target": "es5",
        "moduleResolution": "node",
        "removeComments": true,
	"strict": true,
	"noUnusedLocals": true,
	"noUnusedParameters": false,
	"noImplicitReturns": true,
	"noFallthroughCasesInSwitch": true,
	"strictFunctionTypes": false,
	"allowSyntheticDefaultImports": true,
	"esModuleInterop": true
    },
    "exclude": [
        "node_modules"
    ]
}
